var reverseStr = function(s, k) {
  let arr = [...s];
  for (let i = 0; i < arr.length; i += 2 * k) {
    let a = i;
    let b = i + k - 1;

    while (a < b) {
      if (arr.find((b) => b == -1)) {
        b--;
      }

      [arr[a], arr[b]] = [arr[b], arr[a]];
      a++;
      b--;
    }
  }
  return arr.join("");
};

(reverseStr("abcdefg", 2); //bacdfeg
